.flair:before,<% layout.images.forEach(function (image, index){ %>a[href="<%= image.className.link %>"]:before<% if(layout.images.length != index + 1) { %>,<% } %><% }); %>{background-image: url(%%sprite%%);background-position: -9999px -9999px;content:"";display:inline-block;vertical-align:middle;padding:1px;}<% layout.images.forEach(function (image) { %><% if(image.className.flair) { %>.flair-<%= image.className.flair %>:before,<% } %>a[href="<%= image.className.link %>"]:before{<% if (options.pixelRatio !== 1) { %>background-size:<%= getCSSValue(layout.width) %> <%= getCSSValue(layout.height) %>;<% } %>background-position:<%= getCSSValue(-image.x) %> <%= getCSSValue(-image.y) %>;width:<%= getCSSValue(image.width) %>;height:<%= getCSSValue(image.height) %>;}<% }); %>