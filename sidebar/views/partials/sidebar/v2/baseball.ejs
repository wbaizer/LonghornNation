<% 
var sport = 'baseball'
%>
<h1><a href="/r/BigXII">Baseball</a><% if(data[sport].rankings.primaryTeam) { %>: (<%= data[sport].rankings.primaryTeam.overall %>) <%= data[sport].rankings.primaryTeam.conference %><% } %></h1>
<h4>Big 12 Baseball Standings</h4>   
<table>
       <thead>
           <tr>
               <th></th>
               <th>Team</th>
               <th>Overall</th>
               <th>Conference</th>
           </tr>
       </thead>
       <tbody>
           <% for(var i=0; i < data[sport].rankings.teams.length; i++) { %>
           <tr>
                <td>
                    <% if(data[sport].rankings.teams[i].id == process.env.TEAM_ID) { %><a href="/texas-row"></a><% } %>
                    <a href="<%= teamLink[data[sport].rankings.teams[i].abbreviation] %>"></a>
                </td>
               <td>          
                   <% if(data[sport].rankings.teams[i].id == process.env.TEAM_ID) { %>
                       <strong>
                   <% } %>             
                        
                   <% if(data[sport].rankings.teams[i].rank) { %>
                    <em><%= data[sport].rankings.teams[i].rank %></em>
                <% } %><%= data[sport].rankings.teams[i].name %>
                    </a>
                   <% if(data[sport].rankings.teams[i].id == process.env.TEAM_ID) { %>
                   </strong>
                   <% } %>
               </td>
               <td>
                   <% if(data[sport].rankings.teams[i].id == process.env.TEAM_ID) { %>
                       <strong>
                   <% } %>
                   <%= data[sport].rankings.teams[i].overall %>
                   <% if(data[sport].rankings.teams[i].id == process.env.TEAM_ID) { %>
                       </strong>
                   <% } %>
               </td>
               <td>
                   <% if(data[sport].rankings.teams[i].id == process.env.TEAM_ID) { %>
                       <strong>
                   <% } %>
                       <%= data[sport].rankings.teams[i].conference %>
                   <% if(data[sport].rankings.teams[i].id == process.env.TEAM_ID) { %>
                   </strong>
                   <% } %>
               </td>
           </tr>
           <% } %>
       </tbody>
   </table>
   <% if(data[sport].schedule.post.length > 0) { %>
        <h4>Post Season</h4>
        <%- include('../views/partials/sidebar/v2/partials/schedule', { type: 'post', sport: 'baseball', data }) %>
   <% }%>
   <h4>Regular Season</h4>
   <%- include('../views/partials/sidebar/v2/partials/schedule', { type: 'regular', sport: 'baseball', data }) %>